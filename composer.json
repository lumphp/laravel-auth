{
    "name": "lumphp/laravel-auth",
    "description": "The Illuminate Auth package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-http": "^8.0",
        "lumphp/laravel-macroable": "^8.0",
        "lumphp/laravel-queue": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Auth\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "lumphp/laravel-console": "Required to use the auth:clear-resets command (^8.0).",
        "lumphp/laravel-queue": "Required to fire login / logout events (^8.0).",
        "lumphp/laravel-session": "Required to use the session based guard (^8.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
